<p id="notice"><%= notice %></p>

<h1>Identities</h1>

<table>
  <thead>
    <tr>
      <th>Typeannonceur</th>
      <th>Name</th>
      <th>Country</th>
      <th>City</th>
      <th>Adress</th>
      <th>Zip</th>
      <th>About</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @identities.each do |identity| %>
      <tr>
        <td><%= identity.typeannonceur %></td>
        <td><%= identity.name %></td>
        <td><%= identity.country %></td>
        <td><%= identity.city %></td>
        <td><%= identity.adress %></td>
        <td><%= identity.zip %></td>
        <td><%= identity.about %></td>
        <td><%= identity.user_id %></td>
        <td><%= link_to 'Show', identity %></td>
        <td><%= link_to 'Edit', edit_identity_path(identity) %></td>
        <td><%= link_to 'Destroy', identity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Identity', new_identity_path %>
